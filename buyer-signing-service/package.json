{
  "name": "buyer-signing-service",
  "version": "1.0.0",
  "description": "Buyer Signing Service",
  "author": "Wibson Development Team",
  "main": "src/app.js",
  "scripts": {
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "start": "babel-node src/index.js",
