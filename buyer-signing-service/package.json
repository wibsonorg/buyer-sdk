    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "scripts/publish.sh $npm_package_name"
  },
  "dependencies": {
    "aws-sdk": "^2.188.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.0.0",
    "elliptic": "^6.4.1",
    "eth-crypto": "^1.2.3",
    "ethereumjs-tx": "^1.3.6",
    "ethereumjs-util": "^5.2.0",
    "express": "^4.16.3",
    "express-boom": "^2.0.0",
    "glob": "^7.1.2",
    "helmet": "^3.12.1",
    "level": "^4.0.0",
    "morgan": "1.9.1",
    "node-forge": "^0.7.1",
    "openpgp": "3.1.0",
    "read": "1.0.7",
    "safe-buffer": "^5.1.2",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^3.0.10",
