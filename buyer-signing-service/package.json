    "web3-utils": "1.0.0-beta.34",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "ganache-cli": "^6.1.3",
    "mocha": "^5.2.0",
    "nyc": "12.0.2",
    "sinon": "^6.1.0",
    "supertest": "^3.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 70,
    "cache": false,
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "src/utils/logger.js"
    ]
