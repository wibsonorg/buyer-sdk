    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "raw-loader": "0.5.1",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "dotenv": "^2.0.0",
    "file-saver": "^1.3.3",
    "js-base64": "^2.3.2",
    "memoizee": "^0.4.11",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "ramda": "^0.24.1",
    "react": "^15.3.0",
    "react-cookie": "^3.0.4",
    "react-dom": "^15.3.0",
    "react-modal": "^2.3.2",
    "react-redux": "^5.0.6",
    "react-refetch": "^1.0.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-select": "^2.1.0",
    "recompose": "^0.25.1",
    "redux": "^3.7.2",
    "redux-act": "^1.4.1",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "store": "^2.0.12",
    "svg-react-loader": "^0.4.5",
    "truffle-contract": "^3.0.0",
    "universal-cookie": "^3.0.4",
    "url": "^0.11.0"
  },
  "scripts": {
    "start": "DEPLOY_ENVIRONMENT=development PORT=3001 ACCOUNT_INDEX=1 node scripts/start.js",
    "start-metamask": "DEPLOY_ENVIRONMENT=development PORT=3001 node scripts/start.js",
    "build": "NODE_ENV=production DEPLOY_ENVIRONMENT=production node scripts/build.js",
    "build-production": "NODE_ENV=production DEPLOY_ENVIRONMENT=production node scripts/build.js",
    "build-staging": "NODE_ENV=staging DEPLOY_ENVIRONMENT=staging node scripts/build.js",
    "build-develop": "NODE_ENV=development DEPLOY_ENVIRONMENT=development node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --coverage --env=jsdom",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "scripts/publish.sh $npm_package_name",
    "preinstall": "cd ../base-app && npm install"
  },
  "proxy": "http://localhost:8080",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
